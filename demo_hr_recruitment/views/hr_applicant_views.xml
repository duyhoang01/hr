<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="crm_case_form_view_job_inherit" model="ir.ui.view">
            <field name="name">crm.case.form.view.job.inherit</field>
            <field name="model">hr.applicant</field>
            <field name="inherit_id" ref="hr_recruitment.crm_case_form_view_job"/>
            <field name="arch" type="xml">

                <xpath expr="//sheet/group" position="inside">
                    <group string="Interview info">
                        <field name="interview_start_time"/>
                        <field name="interview_range_time"/>
                        <field name="interview_address"/>
                        <field name="interview_note"/>
                    </group>
                </xpath>

            </field>
        </record>
    </data>
</odoo>